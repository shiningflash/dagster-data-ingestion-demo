{
  "data_sources": [
    {
      "id": "openmeteo_berlin",
      "name": "Open-Meteo Berlin Weather",
      "enabled": true,
      "api_type": "openmeteo",
      "api_config": {
        "base_url": "https://api.open-meteo.com/v1/forecast",
        "latitude": 52.52,
        "longitude": 13.405,
        "location_name": "Berlin",
        "hourly_params": ["temperature_2m", "relative_humidity_2m", "wind_speed_10m"],
        "daily_params": ["temperature_2m_max", "temperature_2m_min"],
        "timezone": "Europe/Berlin",
        "forecast_days": 1
      },
      "schedule": {
        "interval": "hourly",
        "cron": "0 * * * *",
        "start_time": "2024-01-01T00:00:00",
        "enabled": true
      },
      "database": {
        "schema": "weather",
        "table": "berlin_weather_data",
        "columns": {
          "timestamp": "TIMESTAMP",
          "temperature_2m": "FLOAT",
          "relative_humidity_2m": "FLOAT", 
          "wind_speed_10m": "FLOAT",
          "latitude": "FLOAT",
          "longitude": "FLOAT",
          "location_name": "VARCHAR(100)"
        },
        "primary_key": ["timestamp", "location_name"],
        "indexes": ["timestamp", "location_name"]
      },
      "data_retention": {
        "days": 365,
        "cleanup_enabled": true
      }
    },
